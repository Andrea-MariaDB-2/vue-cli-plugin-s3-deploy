{
  "name": "vue-cli-plugin-s3-deploy",
  "version": "4.0.0-rc5",
  "description": "A vue-cli plugin for deploying your built Vue app to an S3 bucket.",
  "scripts": {
    "test": "npx cross-env AWS_ACCESS_KEY_ID=access-key npx cross-env AWS_SECRET_ACCESS_KEY=secret-key jest --coverage",
    "watch-test": "npx cross-env AWS_ACCESS_KEY_ID=access-key npx cross-env AWS_SECRET_ACCESS_KEY=secret-key jest --watch",
    "build": "trash -f ./*.js && tsc",
    "build-watch": "tsc --watch",
    "prepublish": "npm build",
    "lint": "yarn eslint . --ext .ts",
    "lint:fix": "yarn eslint . --ext .ts --fix"
  },
  "keywords": [
    "vue",
    "vuejs",
    "cli",
    "plugin",
    "s3",
    "aws",
    "cloudFront",
    "deploy",
    "deployment"
  ],
  "main": "./index.js",
  "homepage": "https://github.com/multiplegeorges/vue-cli-plugin-s3-deploy",
  "author": {
    "name": "Georges Gabereau",
    "email": "s3-deploy@georg.es",
    "url": "https://georg.es"
  },
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@hapi/joi": "^16.1.4",
    "@vue/cli-shared-utils": "^4.2.3",
    "aws-sdk": "^2.225.1",
    "es6-promise-pool": "^2.5.0",
    "fs-extra": "9.0.0",
    "globby": "^8.0.1",
    "lodash": "^4.17.11",
    "micromatch": "^4.0.2",
    "mime-types": "^2.1.18",
    "trash-cli": "^3.0.0"
  },
  "devDependencies": {
    "@types/globby": "^9.1.0",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^14.6.4",
    "@typescript-eslint/eslint-plugin": "3",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "7",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-with-typescript": "^19.0.1",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "2",
    "eslint-plugin-node": "11",
    "eslint-plugin-promise": "4",
    "eslint-plugin-standard": "4",
    "jest": "25.2.4",
    "prettier": "^2.1.1",
    "typescript": "^4.0.2"
  },
  "files": [
    "bucket.js",
    "configuration.js",
    "connection.js",
    "deployer.js",
    "generator.js",
    "index.js",
    "helper.js",
    "post-tasks.js",
    "prompts.js",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "jest": {
    "verbose": true,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "test_assets"
    ]
  }
}
