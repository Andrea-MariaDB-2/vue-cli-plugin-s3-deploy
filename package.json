{
  "name": "vue-cli-plugin-s3-deploy",
  "version": "4.0.0-rc3",
  "description": "A vue-cli plugin for deploying your built Vue app to an S3 bucket.",
  "scripts": {
    "test": "npx cross-env AWS_ACCESS_KEY_ID=access-key npx cross-env AWS_SECRET_ACCESS_KEY=secret-key jest --coverage",
    "watch-test": "npx cross-env AWS_ACCESS_KEY_ID=access-key npx cross-env AWS_SECRET_ACCESS_KEY=secret-key jest --watch",
    "build": "rm -rf ./dist && babel src/*.js --presets @babel/preset-env --out-dir ./dist",
    "build-watch": "babel src/*.js --watch --presets @babel/preset-env --out-dir ./dist",
    "prepublish": "npm build",
    "lint": "npx standard"
  },
  "keywords": [
    "vue",
    "vuejs",
    "cli",
    "plugin",
    "s3",
    "aws",
    "cloudfront",
    "deploy",
    "deployment"
  ],
  "main": "./dist/index.js",
  "homepage": "https://github.com/multiplegeorges/vue-cli-plugin-s3-deploy",
  "author": {
    "name": "Georges Gabereau",
    "email": "s3-deploy@georg.es",
    "url": "https://georg.es"
  },
  "license": "MIT",
  "private": false,
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "dependencies": {
    "@hapi/joi": "^15.0.1",
    "@vue/cli-shared-utils": "^3.4.0",
    "aws-sdk": "^2.225.1",
    "es6-promise-pool": "^2.5.0",
    "fs-extra": "^7.0.1",
    "globby": "^8.0.1",
    "lodash": "^4.17.11",
    "mime-types": "^2.1.18"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "babel-core": "^7.0.0-bridge",
    "babel-jest": "^23.6.0",
    "jest": "^23.6.0",
    "standardjs": "^1.0.0-alpha"
  },
  "engines": {
    "node": ">=8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "jest": {
    "verbose": true,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "test_assets"
    ]
  }
}
